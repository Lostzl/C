/* Author - Lucas V.
Escreva um algoritmo que imprima todos
os números pares entre X e Y, onde X e Y são dados pelo usuário*/

#include <stdio.h>


int main(){
int x, y, cont;

printf("Entre com o intervalo X e Y, no formato X-Y:");
scanf("%d-%d", &x, &y);

cont = x;
while(cont <= y){

    if ((cont%2) == 0){
        printf("\n\nO Numero %d eh par!\n", cont);
    }
    cont = cont + 1;

}

return 0;
}
