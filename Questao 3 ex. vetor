#include<stdio.h>
#include<stdlib.h>
#include<time.h>


/*Fa√ßa um algoritimo que adicione 5 valores aleatorios a um vetor
sem repeti-los, em seguida ordene-os em ordem crescente e mostre no console.*/


int main(){

// NUM tem que receber a quantidade de valores - 1.
int NUM = 5;
srand(time (NULL));
int A[NUM - 1];
int i, j, k, l, valor;

for(i = 0; i <= 4; i++){
A[i] = 0;
}


for(i = 0; i < NUM; i++){
valor = rand()%10;
A[i] = valor;
	for(j = 0; j < NUM; j++){
		if((A[j] == A[i]) && (i != j)){  //i != 1 para que nao se utilize o i.
                A[i] = (rand()%10) + 1;
                j = 0;
                printf("\nNumeros Iguais\n\n");
		}
	}

}

for(i = 0; i < NUM; i++){
printf("%d\n", A[i]);
}
system("pause");
return 0;
}

